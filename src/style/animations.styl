@keyframes scroll{
	0%{
		background-position:0% 0%
	}
	100%{
		background-position:0 -768px
	}
}

@keyframes pulse{
	0%{
		color:#ffe6a5;
	}
	25%{
		color: #507d8f;
	}
	50%{
		color: #94b8ab;
	}
	75%{
		color: #507d8f;
	}
	100%{
		color:#ffe6a5;
	}
}

@keyframes play {
   from { background-position:    0px; }
     to { background-position: -500px; }
}

@keyframes glide {
	0%{margin-left:10px;}
	50%{ margin-left:20px;}
	100%{margin-left:10px;}
}


@keyframes likeus {
	0%{transform: scale(1.3);}
	50%{ transform: scale(1.4);}
	100%{
		transform: scale(1.3);
	}

}

@keyframes rotate
	0%
		transform: rotate(0deg)
	50%
		transform: rotate(180deg)
	100%
		transform: rotate(360deg)
@keyframes rotate2
	0%
		transform: rotate(0deg)
	50%
		transform: rotate(-180deg)
	100%
		transform: rotate(-360deg)
@keyframes shrink
	0%
		transform: scale(0.8)
	50%
		transform: scale(1)
	100%
		transform: scale(0.8)

@keyframes logo
	0%
		background-position 0 30px
	50%
		background-position 0 40px
	100%
		background-position 0 30px

@keyframes you
	0%
		background-position 0 5px
	50%
		background-position 0 15px
	100%
		background-position 0 5px
