#livefeed
	margin 0 0 3em 0
	height: 50px;
	.pod
		width 64px
		height 64px
		background url('../img/pod.png')
		display inline-block
		animation rotate 15s linear infinite
	.data.visible
		visibility: visible;
	.data
		visibility hidden
		display inline-block
		text-align left
		min-height 64px
		vertical-align: top;
		padding 10px 0 0 2em
		p
		color c4

		.ship
			color c3
		.msg
			color c2
			max-width 350px
			white-space: nowrap;
			overflow: hidden;
			text-overflow: ellipsis;



